<Window x:Class="Pharmacy.INST.DissolutionClient.pages.modal.SetSpeedModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pharmacy.INST.DissolutionClient.pages.modal"
        mc:Ignorable="d"
        Title="设置转速" Height="180" Width="275" ResizeMode="NoResize" SizeToContent="Manual" WindowStartupLocation="CenterScreen" Topmost="True">
    <Canvas>
        <Label Content="转速：" Canvas.Left="19" Canvas.Top="38" FontSize="16"/>
        <ComboBox x:Name="TB_SWIVEL" IsEditable="True" IsTextSearchEnabled="false" Canvas.Left="75" Canvas.Top="40" Width="87" FontSize="25" Height="40">
            <ComboBoxItem>50</ComboBoxItem>
            <ComboBoxItem>100</ComboBoxItem>
            <ComboBoxItem>150</ComboBoxItem>
            <ComboBoxItem>200</ComboBoxItem>
        </ComboBox>
        <Label Content="(rpm)" Canvas.Left="15" Canvas.Top="60" FontSize="16"/>
        <Button x:Name="BTN_CONFIRM" Style="{StaticResource DynamicButton}"  Content="确定" Canvas.Left="173" Canvas.Top="40" Width="67" Height="40" Click="BTN_CONFIRM_Click"/>

    </Canvas>
</Window>
