        <Button x:Name="INM_REGISTER" Content="注册" Style="{StaticResource DynamicButton}"   Canvas.Left="152" Canvas.Top="86" Width="90" Height="26" FontSize="16" Click="INM_REGISTER_Click"/>
        <Button x:Name="INM_CLOSE" Content="关闭" Style="{StaticResource DynamicButton}"   Canvas.Left="248" Canvas.Top="86" Width="70" Height="26" Click="INM_CLOSE_Click"/>
        <Label x:Name="INM_VALIDDATE" Content="" Canvas.Left="20" Canvas.Top="85" Width="145"/>

    </Canvas>
</Window>
