<Window x:Class="Pharmacy.INST.DissolutionClient.pages.modal.ReconnectModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pharmacy.INST.DissolutionClient.pages.modal"
        mc:Ignorable="d"
        x:Name="RMM_MW" Title="重连" Height="200" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Canvas Width="300" Height="130">
        <Button x:Name="RCM_BTN_CONNECT" Style="{StaticResource DynamicButton}" Content="重新连接" Width="130" Height="40" Grid.Column="2" Canvas.Left="83" Canvas.Top="23" RenderTransformOrigin="0.75,0.85" Click="RCM_BTN_CONNECT_Click"/>
        <Button x:Name="RCM_BTN_EXIT" Style="{StaticResource DynamicButton}" Content="关闭" Width="130" Height="40" Canvas.Left="83" Canvas.Top="82" RenderTransformOrigin="0.75,0.85" Click="RCM_BTN_EXIT_Click"/>
    </Canvas>
</Window>
